<form class="row g-3 form1" onsubmit="sendEmail();">
  <div class="col-md-6">
    <label for="inputname" class="form-label"> Nume </label>
    <input type="text" class="form-control" id="inputname" />
  </div>
  <div class="col-md-6">
    <label for="inputPassword4" class="form-label"> Prenume </label>
    <input type="text" class="form-control" id="inputPassword4" />
  </div>
  <div class="col-12">
    <label for="email1" class="form-label"> Email </label>
    <input type="email" class="form-control" id="email1" value="" />
  </div>
  <div class="col-12">
    <label for="inputAddress" class="form-label"> Mesaj </label>
    <textarea
      type="text"
      class="form-control"
      id="inputAddress"
      placeholder="Scrie mesajul aici..."
    ></textarea>
  </div>

  <div class="col-12">
    <label for="inputState" class="form-label"> Localitate </label>
    <input type="text" class="form-control" id="inputState" />
  </div>

  <div class="col-12">
    <button type="submit" class="hero-btn submit-btn">Trimite</button>
  </div>
</form>
